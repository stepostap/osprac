#include <sys/types.h>
#include <unistd.h>
#include <stdio.h>
#include <stdlib.h>


int main(int argc, char *argv[], char *envp[]) {
	pid_t pid = fork();
	if (pid == -1) {
		printf("Unable to create process");
	} else if (pid == 0) {
		printf("Another process is starting.\n");
		char *file = "sem4-1";
		execle("/bin/cat", "/bin/cat", "lab4.2_ostapenko.c", 0, envp);
		printf("Problems detected, unable to continue.");
		exit(-1);
	} else {
		printf("Parent, ID: %d\n", (int)getpid());
		printf("Child, ID: %d\n", (int)pid);
	}
	return 0;
}