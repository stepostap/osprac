#include <stdio.h>
#include <sys/types.h>
#include <unistd.h>

int main(int argc, char *argv[], char *envp[]) {
	printf("args:\n");
	for (int i = 0; i < argc; i++) {
		printf("%s; ", argv[i]);
	}
	
	printf("\nenvp:\n");
	int i = 0;
	while (envp[i] != NULL) {
		printf("%s; ", envp[i]);
		i++;
	}

	return 0;
}